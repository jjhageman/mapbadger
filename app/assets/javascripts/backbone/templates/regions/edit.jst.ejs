<h1>Edit region</h1>

<form id="edit-region" name="region">
  <div class="field">
    <label for="name"> name:</label>
    <input type="text" name="name" id="name" value=<%= name %> >
  </div>

  <div class="field">
    <label for="fipscode"> fipscode:</label>
    <input type="text" name="fipscode" id="fipscode" value=<%= fipscode %> >
  </div>

  <div class="field">
    <label for="coords"> coords:</label>
    <input type="text" name="coords" id="coords" value=<%= coords %> >
  </div>

  <div class="actions">
    <input type="submit" value="Update Region" />
  </div>

</form>


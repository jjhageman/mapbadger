<div id="map-container">
  <div class="well">
    <button id="select-regions" class="btn primary">Select</button>
    <button id="reset-map" class="btn">Reset</button>
    <button id="save-select" class="btn">Save</button>
  </div>
  <div id="map-canvas"></div>
</div>

<script type="text/javascript">
  $(function() {
    // Blog is the app name
    window.router = new Mapbadger.Routers.RegionsRouter({regions: <%= @regions.to_json.html_safe -%>});
    Backbone.history.start();
  });
</script>



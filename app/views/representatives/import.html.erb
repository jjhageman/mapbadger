<ul class="tabs">
  <%= tab_li 'Opportunities', import_opportunities_path %>
  <%= tab_li 'Teams', import_representatives_path %>
</ul>

<h1>New representatives</h1>

<%= form_for :upload do |f| %>
  <fieldset>
    <legend>Representatives File Import</legend>
    <div class="clearfix">
      <%= f.label :csv, 'CSV File' %>
      <div class="input">
        <%= f.file_field :csv, :class => 'input-file' %>
      </div>
    </div>
    <div class="actions">
      <%= f.submit 'Import', :class => 'btn primary' %>
    </div>
  </fieldset>
<% end %>
<hr />
<%= render 'form' %>


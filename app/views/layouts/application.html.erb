        <!--[if (gt IE 9)|!(IE)]><!--> <html lang="en" class="no-js"> <!--<![endif]-->
          <head>
            <meta charset="utf-8">

            <title>Mapbadger</title>
            <%= stylesheet_link_tag    "application" %>
            <%= javascript_include_tag "application" %>
            <%= csrf_meta_tags %>
          </head>
          <body>
            <%= render 'shared/nav_bar' %>
            <div class="container">
              <div class="content">
                <% flash.each do |key, msg| %>
                  <%= content_tag :div, msg, :class => "alert alert-#{key}" unless msg.blank? %>
                <% end %>
                <%= yield %>
              </div>
            </div>
          </body>
        </html>



